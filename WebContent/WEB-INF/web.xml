<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>DentAssist</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>Login.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>Servlets.Login.Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>findPatients</servlet-name>
    <servlet-class>Servlets.Patients.Patients</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>findPatients</servlet-name>
    <url-pattern>/Patients.find</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>addPatients</servlet-name>
    <servlet-class>Servlets.Patients.Patients</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>addPatients</servlet-name>
    <url-pattern>/AddPatient.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>deletePatients</servlet-name>
    <servlet-class>Servlets.Patients.Delete</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>deletePatients</servlet-name>
    <url-pattern>/DeletePatient.do</url-pattern>
  </servlet-mapping>
</web-app>